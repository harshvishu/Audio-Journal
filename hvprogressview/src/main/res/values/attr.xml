<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressView">
        <attr name="progressColor" format="color"/>
        <attr name="backgroundColor" format="color"/>
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        <attr name="headerTextSize" format="dimension"/>
        <attr name="headerTextColor" format="color"/>
        <attr name="startAngle" format="float"/>
        <attr name="endAngle" format="float"/>
        <attr name="sweepAngle" format="float"/>
        <attr name="progressWidth" format="dimension"/>
        <attr name="progressWidthMultiplier" format="float"/>
        <attr name="drawable" format="reference"/>
        <attr name="knobOffset" format="float"/>
        <attr name="knobRadius" format="integer"/>
        <attr name="cap" format="enum">
            <enum name="round" value="1"/>
            <enum name="square" value="2"/>
        </attr>
    </declare-styleable>

    <attr name="ProgressViewStyle" format="reference"/>
</resources>